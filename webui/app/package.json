    "choo": "^6.13.3",
    "pretty-bytes": "^5.2.0",
    "sheetify": "^7.4.0",
    "sheetify-nested": "^1.0.2"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "choo-devtools": "^3.0.0",
    "watchify": "^3.11.1"
  }
}
